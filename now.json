{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@now/node-server"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/lights-status",
      "dest": "/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/lights-on",
      "dest": "/index.js",
      "methods": ["GET"]
    },{
      "src": "/lights-off",
      "dest": "/index.js",
      "methods": ["GET"]
    }
  ]
}